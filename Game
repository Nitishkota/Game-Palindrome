
#include <stdio.h>
#include<stdlib.h>

int main()
{
    
    char name[23];
    char age[2];
    char course[20];
    
    printf("Enter the n1");
    scanf("%s", name);
    printf("Enter the n2");
    scanf("%s", age);
    printf("Enter the n3");
    scanf("%s", course);
    
    printf("My name is %s/n", name);
    printf(" and And my age is %s/n", age);
    printf("And my course at UNM is %s/n", course);
    
    
    
    

    return 0;
}



